					<key>beginCaptures</key>
					<dict>
						<key>0</key>
						<dict>
							<key>name</key>
							<string>punctuation.definition.string.begin.cfml</string>
						</dict>
					</dict>
					<key>contentName</key>
					<string>meta.toc-list.query.cfml</string>
